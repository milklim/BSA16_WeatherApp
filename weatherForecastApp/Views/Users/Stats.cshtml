@model IEnumerable<weatherForecastApp.Models.History>

@{
    ViewBag.Title = "Статистика";
}

<h1>Вы просматривали:</h1>
<span>
    <img src="~/Content/trash.png" alt="Очистить" />
    @Html.ActionLink("Очистить историю", "ClearHistory", new { }, new { @class = "text-danger" })
</span>
<ul class="list-group">
    @foreach (var item in Model)
    {
        <li class="list-group-item"> @item.Date   -   @item.city.name</li>
    }
</ul>   

